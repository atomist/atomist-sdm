    "@atomist/sdm-pack-issue": "^1.2.2",
    "@atomist/sdm-pack-k8s": "^1.10.0",
    "@atomist/sdm-pack-node": "1.2.0-master.20190921230202",
    "@atomist/sdm-pack-s3": "^0.5.0",
    "@atomist/sdm-pack-spring": "^2.0.1",
    "@atomist/slack-messages": "^1.1.1",
